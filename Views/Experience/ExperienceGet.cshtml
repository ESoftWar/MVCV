@model MVCV.Models.Entity.TBL_EXPERIENCES
@{
    ViewBag.Title = "ExperienceGet";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4><b>New Experience Update Page</b></h4>
<br />
<form method="post">
    @Html.Label("Experience ID")
    @Html.TextBoxFor(x => x.ID, new { @class = "form-control" })
    <br />
    @Html.Label("Experience Title")
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    <br />
    @Html.Label("Experience Subheading")
    @Html.TextBoxFor(x => x.Subheading, new { @class = "form-control" })
    <br /> 
    @Html.Label("Experience Description")
    @Html.TextAreaFor(x => x.Description, new { @class = "form-control" ,@rows = "6" })
    <br />
    @Html.Label("Experience Date")
    @Html.TextBoxFor(x => x.Date, new { @class = "form-control" })
    <br />
    
    <button class="btn btn-outline-success">Update</button>
</form>

